{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
        "files": [
            "/favicon.png",
            "/index.html"
        ],
        "versionedFiles": [
            "/*.bundle.css",
            "/*.bundle.js",
            "/*.chunk.js"
        ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
        "files": ["/assets/**"],
        "urls": ["/icons/**"]
    }
  }],
  "dataGroups": [{
    "name": "api-performance",
    "urls": [
        "/",
        "/details/**"],
    "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "2d",
        "timeout": "3s"
    }
  }]
}